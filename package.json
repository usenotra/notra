{
  "name": "turbo",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "dotenv -- turbo run check-types"
  },
  "devDependencies": {
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.7.4",
    "turbo": "^2.7.5"
  },
  "engines": {
    "node": "24.11.1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "bun@1.3.6"
}
