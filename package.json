{
  "name": "turbo",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "lint": "dotenv -- turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "dotenv -- turbo run check-types",
    "db:generate": "dotenv -- bunx drizzle-kit generate --config packages/db/drizzle.config.ts",
    "db:migrate": "dotenv -- bunx drizzle-kit migrate --config packages/db/drizzle.config.ts",
    "db:push": "dotenv -- bunx drizzle-kit push --config packages/db/drizzle.config.ts",
    "db:studio": "dotenv -- bunx drizzle-kit studio --config packages/db/drizzle.config.ts",
    "db:drop": "dotenv -- bunx drizzle-kit drop --config packages/db/drizzle.config.ts",
    "db:pull": "dotenv -- bunx drizzle-kit pull --config packages/db/drizzle.config.ts",
    "db:seed:automation": "dotenv -- bun packages/db/scripts/seed-automation.ts",
    "db:seed:posts": "dotenv -- bun packages/db/scripts/seed-posts.ts",
    "db:seed": "bun run db:seed:posts && bun run db:seed:automation"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": "24.11.1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "bun@1.3.6",
  "dependencies": {
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.4",
    "@shikijs/transformers": "^3.21.0",
    "@xyflow/react": "^12.10.0",
    "ai": "^6.0.39",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "nanoid": "^5.1.6",
    "next": "^16.1.3",
    "react-icons": "^5.5.0",
    "shiki": "^3.21.0",
    "streamdown": "^2.0.1",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1"
  }
}
